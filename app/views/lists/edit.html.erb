<h1>Editing Book</h1>

<%= form_with model: @list,:url => {:action => :update} do |f| %>

 <h4>Title</h4>
 <%= f.text_field :title %>

 <h4>Body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Update Book' %>
<%end%>
  <p><%= link_to 'show', show_path(@list.id) %> | <%= link_to 'Back', lists_path %></p>